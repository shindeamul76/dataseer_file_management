{
  "name": "file_management",
  "version": "1.0.0",
  "description": "",
  "main": "main/index.js",
  "scripts": {
    "start:ts": "ts-node-dev --respawn --transpile-only --exit-child ./src/main/index.ts",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "tsc -p tsconfig-build.json --watch",
    "start": "node dist/main/index.js",
    "dev": "yarn run build && nodemon -L --watch ./dist ./dist/main/index.js",
    "start:dev": "concurrently --kill-others-on-fail \"yarn run build:watch\" \"yarn run dev\""
  },
  "keywords": [
    "File Management",
    "Dataseers Tech",
    "Typescript"
  ],
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@contollers": "dist/contollers",
    "@routes": "dist/routes",
    "@utils": "dist/utils",
    "@lib": "dist/lib",
    "@main": "dist/main"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "concurrently": "^8.2.2",
    "husky": "^9.0.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
